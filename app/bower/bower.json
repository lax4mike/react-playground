{
  "name": "es6-playground",
  "private": true,
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components"
  ],
  "dependencies": {
    "codemirror": "~5.0.0",
    "kefir": "~1.1.0",
    "jquery": "~2.1.3",
    "babel": "./babel-browser-4.7.16.js",
    "browserpolyfill": "./browser-polyfill.js",
    "svg4everybody": "https://github.com/jonathantneal/svg4everybody.git#~1.0.0",
    "react": "~0.13.3",
    "codemirror-jsx": "./codemirror-jsx.js"
  },
  "overrides": {
    "codemirror": {
      "main": [
        "lib/codemirror.js",
        "mode/javascript/javascript.js",
        "mode/xml/xml.js",
        "mode/css/css.js",
        "mode/htmlmixed/htmlmixed.js",
        "lib/codemirror.css",
        "theme/eclipse.css"
      ]
    },
    "codemirror-jsx": {
      "dependencies": {
        "codemirror": "~5.0.0"
      }
    },
    "svg4everybody": {
      "main": "svg4everybody.min.js"
    }
  }
